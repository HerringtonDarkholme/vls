{
  "name": "vls",
  "version": "0.0.1",
  "description": "vue language service",
  "main": "./lib/htmlLanguageService.js",
  "typings": "./lib/htmlLanguageService",
  "repository": {
    "type": "git",
    "url": "https://github.com/octref/vls.git"
  },
  "license": "MIT",
  "devDependencies": {
    "mocha": "^2.4.5",
    "typescript": "^2.1.5",
    "cpy-cli": "^1.0.1",
    "@types/node": "^6.0.51",
    "@types/mocha": "^2.2.33"
  },
  "dependencies": {
    "vscode-languageserver-types": "^3.0.3",
    "vscode-nls": "^2.0.2",
    "vscode-uri": "^1.0.0"
  },
  "scripts": {
    "compile": "tsc -p ./src && cpy ./src/beautify/*.js ./lib/beautify",
